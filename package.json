{
  "name": "@placekit/placekit-js",
  "version": "0.0.1",
  "author": "Nicolas Torres <nico@placekit.io>",
  "description": "PlaceKit JavaScript client",
  "license": "MIT",
  "homepage": "https://github.com/placekit/placekit-js#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/placekit/placekit-js.git"
  },
  "bugs": {
    "url": "https://github.com/placekit/placekit-js/issues"
  },
  "main": "dist/index.cjs.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "jsdelivr": "./dist/index.umd.js",
  "unpkg": "./dist/index.umd.js",
  "browser": {
    "./index.js": "./dist/index.umd.js"
  },
  "exports": {
    ".": {
      "require": "./dist/index.cjs.js",
      "import": "./dist/index.esm.js"
    }
  },
  "files": [
    "dist",
    "LICENSE"
  ],
  "scripts": {
    "clear": "rimraf dist",
    "build:lib": "rollup -c",
    "build:types": "tsc --allowJs -d --emitDeclarationOnly --removeComments src/index.js -outDir dist",
    "build": "npm run build:lib && npm run build:types",
    "lint": "eslint ./src",
    "format": "eslint ./src --fix"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^22.0.0",
    "eslint": "^8.17.0",
    "eslint-config-google": "^0.14.0",
    "rimraf": "^3.0.2",
    "rollup": "^2.75.6",
    "rollup-plugin-cleanup": "^3.2.1",
    "rollup-plugin-terser": "^7.0.2",
    "typescript": "^4.7.4"
  }
}
