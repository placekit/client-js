{
  "name": "@placekit/client-js",
  "version": "1.0.0",
  "author": "PlaceKit <support@placekit.io>",
  "description": "PlaceKit JavaScript client",
  "license": "MIT",
  "homepage": "https://github.com/placekit/client-js#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/placekit/client-js.git"
  },
  "bugs": {
    "url": "https://github.com/placekit/client-js/issues"
  },
  "types": "./dist/placekit.d.ts",
  "module": "./dist/placekit.esm.mjs",
  "main": "./dist/placekit.cjs.js",
  "browser": "./dist/placekit.umd.js",
  "exports": {
    ".": {
      "types": "./dist/placekit.d.ts",
      "require": "./dist/placekit.cjs.js",
      "import": "./dist/placekit.esm.mjs"
    }
  },
  "files": [
    "dist",
    "LICENSE"
  ],
  "scripts": {
    "clear": "rimraf ./dist",
    "build": "rollup -c",
    "lint": "eslint ./src",
    "format": "eslint ./src --fix",
    "test": "jest --runInBand --detectOpenHandles --forceExit --roots ./src",
    "test:watch": "jest --runInBand --detectOpenHandles --forceExit --roots ./src --onlyFailures --watch",
    "test:ci": "jest --runInBand --detectOpenHandles --forceExit --roots ./src --ci",
    "install:examples": "find examples -maxdepth 2 -name package.json -not -path \"*/node_modules/*\" -execdir npm i \\;"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^24.0.1",
    "eslint": "^8.36.0",
    "eslint-config-google": "^0.14.0",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "jest-fetch-mock": "^3.0.3",
    "rimraf": "^4.4.0",
    "rollup": "^3.20.0",
    "rollup-plugin-cleanup": "^3.2.1",
    "rollup-plugin-copy": "^3.4.0"
  }
}
