{
  "name": "@placekit/placekit-js",
  "version": "0.0.1",
  "author": "Nicolas Torres <nico@placekit.io>",
  "description": "PlaceKit JavaScript client",
  "license": "MIT",
  "homepage": "https://github.com/placekit/placekit-js#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/placekit/placekit-js.git"
  },
  "bugs": {
    "url": "https://github.com/placekit/placekit-js/issues"
  },
  "types": "./dist/placekit.d.ts",
  "module": "./dist/placekit.esm.js",
  "main": "./dist/placekit.cjs.js",
  "browser": "./dist/placekit.umd.js",
  "exports": {
    ".": {
      "require": "./dist/placekit.cjs.js",
      "import": "./dist/placekit.esm.js"
    }
  },
  "files": [
    "dist",
    "LICENSE"
  ],
  "scripts": {
    "clear": "rimraf ./dist",
    "build": "rollup -c",
    "lint": "eslint ./src",
    "format": "eslint ./src --fix",
    "test": "jest --runInBand --detectOpenHandles --forceExit --roots ./src",
    "test:watch": "jest --runInBand --detectOpenHandles --forceExit --roots ./src --onlyFailures --watch",
    "test:ci": "jest --runInBand --detectOpenHandles --forceExit --roots ./src --ci"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^22.0.0",
    "eslint": "^8.17.0",
    "eslint-config-google": "^0.14.0",
    "jest": "^28.1.3",
    "jest-environment-jsdom": "^28.1.3",
    "jest-fetch-mock": "^3.0.3",
    "rimraf": "^3.0.2",
    "rollup": "^2.75.6",
    "rollup-plugin-cleanup": "^3.2.1",
    "rollup-plugin-copy": "^3.4.0",
    "rollup-plugin-terser": "^7.0.2"
  }
}
